<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Admin:</strong>
  <%= @post.admin.email %>
</p>


<%= link_to 'Edit', edit_admin_post_path(@post), class: 'btn btn-xs btn-default' %>
<%= link_to 'Destroy', admin_post_path(@post), class: 'btn btn-xs btn-default', method: :delete, data: { confirm: 'Are you sure?' } %>
<%= link_to 'Back', admin_posts_path, class: 'btn btn-xs btn-default' %>
